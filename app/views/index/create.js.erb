<% $stderr<< @need %>
$('#need_text').val("")
$('#needs').prepend("<div class='item' id='<%= @need[1] %>'></div>")
$('#<%= @need[1] %>').html("<div class='info'><%= @need[0][:date] %>: </div>\
  <div class='text' contenteditable spellcheck='false'><%= @need[0][:text] %></div>\
  <span class='shadow'><%= @need[0][:text] %></span>" +
  '<div class="controls">\
    <%= link_to '', '#', class: 'delete_need' %>\
    <%= link_to '', '#', class: 'complete_need' %>\
  </div>')
$('#<%= @need[1] %>').settings()